# ============================================================
# General
# ============================================================
LOG_LEVEL=info
APP_API_KEY=change-me-before-deployment

# ============================================================
# Paperless-ngx
# ============================================================
PAPERLESS_BASE_URL=http://paperless:8000
PAPERLESS_API_TOKEN=your-paperless-api-token-here
PAPERLESS_TIMEOUT=30
# Fallback owner_id for documents where owner=null (e.g. admin-created docs).
# Set to your Paperless-ngx admin user ID. Leave unset to skip ownerless documents.
# PAPERLESS_DEFAULT_OWNER_ID=1

# ============================================================
# Qdrant
# ============================================================
QDRANT_BASE_URL=http://qdrant:6333
QDRANT_COLLECTION=paperless_docs
QDRANT_TIMEOUT=30
QDRANT_API_KEY=your-qdrant-api-key-here

# ============================================================
# Embedding model (Phase II)
# ============================================================
EMBED_BASE_URL=http://ollama:11434
EMBED_MODEL=nomic-embed-text
EMBED_TIMEOUT=60

# ============================================================
# LLM / Agent (Phase III)
# ============================================================
LLM_BASE_URL=http://ollama:11434
LLM_MODEL=llama3.2
LLM_TIMEOUT=120
